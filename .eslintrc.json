{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "rules": {
        "strict": 0,
        "indent": ["error", 4, {
            "VariableDeclarator": 2
        }],
        "no-console": 0,
        "require-jsdoc": 2,
        "valid-jsdoc": 2,
        "space-before-function-paren": 0
    }
}
